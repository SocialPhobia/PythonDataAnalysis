#import modules import csv
import pandas as pd
from matplotlib import pyplot as plt import numpy as np

#array definitions data_array=[] a_array=[] b_array=[]

#open files
with open("./Files/CapstoneZero88ms.CSV") as file_again: for data in file_again:
data_array.append(data.rstrip(",")) readcsv = csv.reader(data_array)

#close files file_again.close()

#append array
for data in readcsv: a_array.append(data)
a_del=np.delete(a_array,0,0)

b_array=np.array(a_del) z88=pd.DataFrame(data=b_array)

#Data Conditioning x =[0,1,2]
for n in x:
z88[n] = z88[n].astype(float) z88=z88.abs()


plt.plot(z88[2]) plt.title("Zero 88ms in") plt.show() plt.plot(z88[1]) plt.title("Zero 88ms out") plt.show()

#df.to_csv("out.csv") """
 
